<h1>Advertisements</h1>
<% @advertisements.each do |advertisement| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= link_to advertisement.title, advertisement %>
      </h4>
      <p><%= number_to_currency(advertisement.price) %></p>
    </div>
  </div>
<% end %>
